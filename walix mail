-- Get Diamond UID
local diamondUID
for Class, Inv in pairs(Inventory) do
    if Class == "Currency" then  
        for uid, Item in pairs(Inv) do
            if Item.id == "Diamonds" then  
                diamondUID = uid
                print("Diamonds UID found:", diamondUID)
                break
            end
        end
    end
end

-- Send to each user if gems are enough
if diamondUID then
    for _, username in ipairs(usernames) do
        if currentGemCount >= amount then
            local args = {
                [1] = username,
                [2] = "Here you go buddy",
                [3] = "Currency",
                [4] = diamondUID,
                [5] = amount
            }

            print("Sending to:", username)
            game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("Mailbox: Send"):InvokeServer(unpack(args))

            currentGemCount -= amount
            task.wait(1)
        else
            warn("Not enough gems left for user:", username)
        end
    end
else
    warn("Diamond UID not found.")
end
